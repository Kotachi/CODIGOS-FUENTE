<Window x:Class="ejer4Etiquetas.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ejer4Etiquetas"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Visibility="Hidden">
        <Button x:Name="cambiar" Content="Cambiar" HorizontalAlignment="Left" Margin="92,221,0,0" VerticalAlignment="Top" Width="75" Click="cambiar_Click"/>
        <Button x:Name="salir" Content="Salir" HorizontalAlignment="Left" Margin="366,221,0,0" VerticalAlignment="Top" Width="75" Click="salir_Click"/>
        <Label x:Name="etiqueta" Content="Ingrese nombre" HorizontalAlignment="Left" Margin="92,123,0,0" VerticalAlignment="Top" Width="152" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="1.378"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label x:Name="label1" Content="Label" HorizontalAlignment="Left" Height="5" Margin="207,206,0,0" VerticalAlignment="Top" Width="5"/>

    </Grid>
</Window>
